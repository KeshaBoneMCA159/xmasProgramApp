<%- include('../partials/header', { currentPage: 'directors' }) %>

<main class="main" id="main">
    <div>
        <% directorData.forEach(director => { %> 
            <figure>
                <!-- Update these to use your actual actor fields (fName, lName, actor_id, etc.) -->
                <figcaption>
                    <a href="/director/<%= director.director_id %>">
                        <%= director.fName %> <%= director.lName %>
                    </a>
                </figcaption>
        </figure>
   <% }) %>
</div>
</main>

<%- include('../partials/footer')%>
